{
  "name": "cli-compiler",
  "version": "0.1.12",
  "description": "Compiler for command line interfaces",
  "author": "muji <noop@xpm.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/freeformsystems/cli-compiler"
  },
  "bugs": {
    "url": "https://github.com/freeformsystems/cli-compiler/issues",
    "email": "muji <noop@xpm.io>"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/freeformsystems/cli-compiler/blob/master/LICENSE"
    }
  ],
  "keywords": [
    "cli",
    "program",
    "command",
    "compiler",
    "markdown"
  ],
  "dependencies": {
    "async": "^0.9.0",
    "circular": "^1.0.5",
    "cli-define": "^0.5.54",
    "cli-env": "^1.0.9",
    "cli-error": "^0.5.7",
    "cli-interface": "^1.0.11",
    "cli-middleware": "0.0.3",
    "cli-util": "^1.1.24",
    "escodegen": "^1.4.1",
    "esprima": "^1.2.2",
    "markzero": "^0.1.29"
  },
  "devDependencies": {
    "chai": "^1.9.1",
    "istanbul": "^0.3.2",
    "mocha": "^1.21.4"
  },
  "engine": [
    "node >= 0.10.21"
  ],
  "scripts": {
    "readme": "mdp --force -v",
    "pretest": "rm -rfv ./target && mkdir -p ./target",
    "test": "istanbul cover _mocha -- -u bdd --recursive --bail --reporter list -A test/spec"
  },
  "mdp": {
    "title": "Compiler",
    "pedantic": true,
    "include": "doc/readme",
    "require": "lib",
    "links": "links.md",
    "toc": "Table of Contents",
    "base": "https://github.com/freeformsystems/cli-compiler",
    "partial": [
      {
        "inc": [
          "introduction.md",
          "install.md",
          "test.md",
          "usage.md"
        ]
      },
      {
        "inc": [
          "license.md"
        ]
      }
    ]
  }
}
